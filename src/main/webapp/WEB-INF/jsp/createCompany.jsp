<script>

jQuery('#country').on('change', function() {
			var cname = this.value;
			var myArray = new Array('Afghanistan','Albania','Algeria','American Samoa','Argentina','Armenia','Australia','Austria','Bahrain','Bangladesh','Belarus','Belgium','Bhutan','Bosnia and Herzegovina','Brazil','Bulgaria','Cambodia','Cape Verde','Chile','China','Christmas','Island','Cocos (Keeling) Island','Colombia','Costa Rica','Croatia','Cuba','Cyprus','Czech Republic','Denmark','Dominican Republic','Ecuador','Egypt','El Salvador','Estonia','Ethiopia','Faroe Islands','Finland','France','Georgia','Germany','Greece','Greenland','Guam','Guatemala','Guinea','Guinea-Bissau','Haiti','Hungary','Iceland','India','Indonesia','Iran','Iraq','Israel','Italy','Jamaica','Japan','Jordan','Kazakhstan','Kenya','Korea','South','Kuwait','Kyrgyzstan','Laos','Lebanon','Lesotho','Liberia','Liechtenstein','Luxembourg','Macedonia','Madagascar','Malaysia','Maldives','Marshall','Islands','Mauritius','Mexico','Micronesia','Mongolia','Morocco','Mozambique','Nepal','New Zealand','Nicaragua','Niger','Nigeria','Norfolk Island','Northern Mariana Islands','Norway','Oman','Pakistan','Palau','Panama','Papua New Guinea','Paraguay','Peru','Philippines','Poland','Portugal','Puerto Rico','Russia','Saudi Arabia','Senegal','Singapore','Slovakia','Slovenia','South Africa','Spain','Sri Lanka','Sudan','Sweden','Switzerland','Svalbard','Taiwan','Tajikistan','Tanzania','Thailand','Trinidad','Tobago','Tunisia','Turkey','Turkmenistan','Ukraine','Uruguay','Virgin Islands','Uzbekistan','Venezuela','Vietnam','Zambia');
			
			if(cname == 'USA'){
				jQuery('#stid').html('State<font color="#FF0000">*</font>');
				jQuery( '#address_state option[value="California"]').attr('selected', 'selected');
				jQuery('#zip_postal').text('Zipcode');
			}else if( jQuery.inArray(cname, myArray) !== -1 ) {
				jQuery('#stid').html('State<font color="#FF0000">*</font>');
				jQuery('#zip_postal').text('Zipcode');
				
				jQuery('#address_city').val('');
				jQuery('#address_zipcode').val('');
			}else{
				jQuery('#stid').html('Province<font color="#FF0000">*</font>');
				jQuery('#zip_postal').text('Postal Code');
				
				jQuery('#address_city').val('');
				jQuery('#address_zipcode').val('');
				
			}
		});
		
jQuery(document).ready(function(){
	
	jQuery("#phone").intlTelInput({
		utilsScript: "https://www.instasubmit.com/wp-content/themes/codify/intl-tel-input-master/build/js/utils.js",
		initialCountry: "us",
		autoPlaceholder: "polite",
		separateDialCode: true,
		formatOnDisplay: false,
		hiddenInput: "full_number"
	});

	jQuery.validator.addMethod('phoneregex', function (value){
		   return /^((\+[1-9]{1,4}[ \-]*)|(\([0-9]{2,3}\)[ \-]*)|([0-9]{2,4})[ \-]*)*?[0-9]{3,4}?[ \-]*[0-9]{3,4}?$/.test(value);
	},);
	
	jQuery.validator.addMethod('emailregex', function (value){
		   return /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/.test(value);
	},);
	
	jQuery("#swpm-createCompany-form").validate({
		rules:{
			email:{
				required: true ,
				emailregex: true,
			},
			password: "required",
			password_re: {
			  equalTo: "#password"
			},
			
			first_name: "required",
			last_name: "required",
			user_name: "required",
			address_street: "required",
			address_city: "required",
			address_zipcode: "required",
			country: "required",
			address_state: "required",
			phone:{ required: true,
					 phoneregex: true,
					},

		},
		messages: {
			email:{
				required: "Please enter Email Address",
				emailregex: "Please enter Valid Email",
			},
			password:"Please enter Password",
			
			first_name:"Please enter First Name",
			last_name:"Please enter Last Name",
			user_name:"Please enter User Name",
			phone:{ required: "Please Enter phone Number",
					 phoneregex: "Please Enter Valid phone Number",},
									
		},

	});
	
	jQuery('#swpm-createCompany-form').submit(function() {
	  
	  	jQuery("#phone_full").val(jQuery("#phone").intlTelInput("getNumber"));
	  	var captcha = jQuery( '#recaptcha' ),
		response = grecaptcha.getResponse();
		if (response.length === 0) {
			jQuery( '.msg-error').text( "reCAPTCHA is mandatory" );
			if( !captcha.hasClass( "error" ) ){
			   captcha.addClass( "error" );
			}
			return false;
		} else {
			jQuery( '.msg-error' ).text('');
			captcha.removeClass( "error" );
		}
	});
	
	jQuery( '#country option[value="USA"]').attr('selected', 'selected');
	jQuery( '#country' ).trigger( "change" );
	setTimeout( function(){ changeState() } , 1000 );
	
	jQuery('#is_mobile').change(function() {
		if(this.checked == true){
		    jQuery("#mobile").val(jQuery("#phone").val());
		}
	    else{
	    	jQuery("#mobile").val("");
	    }
	});
	
	jQuery('#phone').keydown(function (e) {
		 var key = e.charCode || e.keyCode || 0;
		 $text = jQuery(this); 
		 
		 if (key !== 8 && key !== 9) {
			 if ($text.val().length === 0) {
				 $text.val('('+$text.val());
			 }
			 if ($text.val().length === 4) {
				 $text.val($text.val() + ') ');
			 }
			 if ($text.val().length === 9) {
				 $text.val($text.val() + '-');
			 }

		 }

		 return (key == 8 || key == 9 || key == 46 || (key >= 48 && key <= 57) || (key >= 96 && key <= 105));
	});
	
	jQuery('#mobile').keydown(function (e) {
		 var key = e.charCode || e.keyCode || 0;
		 $text = jQuery(this); 
		 
		 if (key !== 8 && key !== 9) {
			 if ($text.val().length === 0) {
				 $text.val('('+$text.val());
			 }
			 if ($text.val().length === 4) {
				 $text.val($text.val() + ') ');
			 }
			 if ($text.val().length === 9) {
				 $text.val($text.val() + '-');
			 }

		 }

		 return (key == 8 || key == 9 || key == 46 || (key >= 48 && key <= 57) || (key >= 96 && key <= 105));
	});
	
});

function changeState(){
	jQuery( '#address_state option[value="California"]').attr('selected', 'selected');
}


jQuery('#address_zipcode').keyup(function() {
        
        var zipcode = jQuery('#address_zipcode').val().substring(0, 5);
         
    	if (zipcode.length == 5 && /^[0-9]+$/.test(zipcode))
    	{
        
          var ajaxurls = 'http://www.namemaxx.com/CustomerAjax?tabid=zipcode';
          jQuery.ajax({
    				url: ajaxurls,
    				data: { action: "getAddressDetailsByZipcode",zipcode:zipcode },
    				type: 'post',
    				success: function (response) {
    					var returnedData = JSON.parse(response);
    					
    					jQuery("#address_city").val(returnedData.city);
    					
    					
    					if(returnedData.country != '' && returnedData.state != ''){
    						populatecountries("country", "address_state", returnedData.country, returnedData.state);
    					}
    					
    					jQuery('#country').removeAttr('selected').filter(returnedData.country).attr('selected', true);
    					jQuery('#country').val(returnedData.country);
    					
    					
    					jQuery('#address_state').removeAttr('selected').filter(returnedData.state).attr('selected', true);
    					jQuery('#address_state').val(returnedData.state);
    					
    				}
    			});
    			
    	}
});

